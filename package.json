{
  "name": "exporoutertv",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "expo prebuild --clean",
    "prebuild:tv": "EXPO_TV=1 expo prebuild --clean",
    "start": "expo start",
    "start:tv": "EXPO_TV=1 expo start",
    "android": "expo run:android",
    "android:tv": "EXPO_TV=1 expo run:android",
    "ios": "expo run:ios",
    "ios:tv": "EXPO_TV=1 expo run:ios",
    "web": "expo start --web",
    "reset-project": "./scripts/reset-project.js",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^6.0.2",
    "expo": "^51.0.31",
    "expo-application": "~5.9.1",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "npm:react-native-tvos@0.76.0-0rc1",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-reanimated": "^3.15.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "^3.34.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^14.1.1",
    "@react-native-tvos/config-tv": "^0.0.10",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "private": true
}
